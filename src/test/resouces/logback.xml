<?xml version="1.0" encoding="UTF-8"?>
<configuration>
	<appender name="appender" class="ch.qos.logback.core.ConsoleAppender">
		<encoder>
			<pattern>%-5level: %msg%n</pattern>
		</encoder>
	</appender>
	<root level="INFO">
		<appender-ref ref="appender" />
	</root>
	<logger name="com.ikueb" level="DEBUG" />
</configuration>